{
  "manifest_version": 3,
  "name": "GeoGuessr: Russian Localization",
  "version": "1.0.0",
  "description": "Переводит интерфейс GeoGuessr на русский язык",
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "action": {
    "default_icon": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "permissions": [],
  "host_permissions": [
    "https://www.geoguessr.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://www.geoguessr.com/*"
      ],
      "js": [
        "js/main.js"
      ],
      "css": [
        "css/main.css"
      ],
      "run_at": "document_start"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "js/inject.js",
        "fonts/Nunito-BlackItalic.ttf"
      ],
      "matches": [
        "https://www.geoguessr.com/*"
      ]
    }
  ]
}
